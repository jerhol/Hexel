{
  "compilerOptions": {
    "target": "ES6",
    "module": "ES6",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "outDir": "./static/js",
    "rootDir": "./assets/js",
    "sourceMap": true
  },
  "include": ["assets/js/**/*"]
}